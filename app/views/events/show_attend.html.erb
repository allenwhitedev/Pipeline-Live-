<!-- Will sort this out once checkin button works -->
<% provide(:title, @event.title) %>
<div class="container">

  <div class="row">
    <aside class="span4">
      <section>
        <%= render 'shared/stats' %>
          </div>
      </section>
    </aside>
    <div class="span8">
      <h3><%= @event.title %> Attendance</h3>
      <% if @users.any? %>
        <ul class="users">
          <%= render @users %>
        </ul>
        <%= will_paginate @users %>
      <% end %>
    </div>
</div>